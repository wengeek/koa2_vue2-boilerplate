{
  "name": "koa2_vue2-boilerplate",
  "version": "1.0.0",
  "main": "server/bin",
  "description": "vuejs 2.0, koajs 2.0, webpack 2.0 boilterplate",
  "author": "wengeek <wenwei897684475@gmail.com>",
  "license": "MIT",
  "keywords": [
    "koajs",
    "vuejs",
    "webpack"
  ],
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint server/*",
    "server": "npm run lint && babel-node server/bin",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js --progress --hide-modules",
    "build:server": "babel server --out-dir app",
    "build": "npm run lint && npm run build:server && npm run build:client",
    "prod": "cross-env NODE_ENV=production node app/bin"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "koa": "2.0.0-alpha.7",
    "koa-bodyparser": "3.2.0",
    "koa-convert": "^1.2.0",
    "koa-log4": "^2.1.0",
    "koa-router": "7.0.1",
    "koa-sendfile": "^2.0.0",
    "koa-static": "3.0.0",
    "vue": "^2.1.4",
    "vue-router": "^2.1.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^0.0.9",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.4",
    "webpack": "2.1.0-beta.27",
    "webpack-merge": "^1.0.2"
  }
}
